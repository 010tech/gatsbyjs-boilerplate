{
  "name": "@010tech/gatsbyjs-boilerplate",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "1.0.0",
  "repository": "git@github.com:010tech/gatsbyjs-boilerplate.git",
  "author": "Gary Lai <garylai1990@gmail.com>",
  "dependencies": {
    "dotenv": "8.2.0",
    "gatsby": "^2.24.79",
    "gatsby-image": "^2.4.21",
    "gatsby-plugin-config": "1.0.7",
    "gatsby-plugin-manifest": "^2.4.34",
    "gatsby-plugin-next-seo": "1.6.1",
    "gatsby-plugin-offline": "^3.2.33",
    "gatsby-plugin-postcss": "3.0.3",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-plugin-sharp": "^2.6.42",
    "gatsby-plugin-sitemap": "2.4.17",
    "gatsby-source-filesystem": "^2.3.34",
    "gatsby-transformer-sharp": "^2.5.17",
    "postcss": "8.1.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet-async": "1.0.7"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "babel-eslint": "10.1.0",
    "commitizen": "4.2.1",
    "conventional-github-releaser": "3.1.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.13.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.1.2",
    "gatsby-plugin-eslint": "2.0.8",
    "husky": "4.3.0",
    "prettier": "2.1.2",
    "standard-version": "9.0.0",
    "tailwindcss": "1.9.4"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint --ext .jsx,.js ./src/",
    "release": "standard-version -s --no-verify",
    "github-release": "conventional-github-releaser -p angular -r 0"
  },
  "bugs": {
    "url": "https://github.com/010tech/gatsbyjs-boilerplate/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
