{
  "name": "@010tech/gatsbyjs-boilerplate",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "1.0.2",
  "repository": "git@github.com:010tech/gatsbyjs-boilerplate.git",
  "authors": ["DBB <qeroer@gmail.com>","Pelic Hsu <pelicwei@gmail.com>","Gary Lai <garylai1990@gmail.com>"],
  "dependencies": {
    "dotenv": "10.0.0",
    "gatsby": "3.14.6",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-config": "1.0.7",
    "gatsby-plugin-manifest": "3.14.0",
    "gatsby-plugin-next-seo": "1.9.0",
    "gatsby-plugin-offline": "4.14.0",
    "gatsby-plugin-postcss": "4.14.0",
    "gatsby-plugin-react-helmet": "4.14.0",
    "gatsby-plugin-sharp": "3.14.3",
    "gatsby-plugin-sitemap": "4.10.0",
    "gatsby-source-filesystem": "3.14.0",
    "gatsby-transformer-sharp": "3.14.0",
    "postcss": "8.4.4",
    "prop-types": "15.7.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet-async": "1.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "babel-eslint": "10.1.0",
    "commitizen": "4.2.4",
    "conventional-github-releaser": "3.1.5",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.4.1",
    "eslint-config-airbnb": "19.0.2",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "gatsby-plugin-eslint": "3.0.0",
    "husky": "7.0.4",
    "prettier": "2.5.1",
    "standard-version": "9.3.2",
    "tailwindcss": "3.0.7"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "lint": "eslint --ext .jsx,.js ./src/",
    "release": "standard-version -s --no-verify",
    "github-release": "conventional-github-releaser -p angular -r 0"
  },
  "bugs": {
    "url": "https://github.com/010tech/gatsbyjs-boilerplate/issues"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
